#define loot_Civilian		(330)
#define loot_Industrial		(341)
#define loot_Police			(346)
#define loot_Military		(356)
#define loot_Medical		(1240)
#define loot_Survivor		(1247)
#define loot_CarIndustrial	(1279)


enum e_loot_data
{
	loottype,
	string[256]
}

new
	LootData[5000][e_loot_data],
	TotalRecords;

ConvertLoot(model, Float:x, Float:y, Float:z, Float:rx, Float:ry, Float:rz)
{
	#pragma unused rx, ry, rz

	new
		type[32],
		rarity,
		id;

	switch(model)
	{
		case loot_Civilian:			type = "loot_Civilian",			id = 7, rarity = 30;
		case loot_Industrial:		type = "loot_Industrial",		id = 6, rarity = 35;
		case loot_Police:			type = "loot_Police",			id = 5, rarity = 40;
		case loot_Military:			type = "loot_Military",			id = 4, rarity = 20;
		case loot_Medical:			type = "loot_Medical",			id = 3, rarity = 35;
		case loot_Survivor:			type = "loot_Survivor",			id = 2, rarity = 10;
		default: printf("ERROR: model %d not convertible.", model);
	}

	format(LootData[TotalRecords][string], 256, "	CreateStaticLootSpawn(%f, %f, %f, %s, %d);\r\n", x, y, z, type, rarity);
	LootData[TotalRecords][loottype] = id;

	TotalRecords++;
}

SaveFile()
{
	new
		File:file;

	SortDeepArray(LootData, loottype, .order = SORT_DESC);

	if(!fexist("output.txt"))
		file = fopen("output.txt", io_write);

	else
		file = fopen("output.txt", io_append);

	if(file)
	{
		for(new i; i < TotalRecords; i++)
		{
			fwrite(file, LootData[i][string]);
		}
	}

	fclose(file);
}
